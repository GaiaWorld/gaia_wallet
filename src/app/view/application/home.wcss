.applicationTop{
    background-color: #fff;
    position: relative;
}
.applTab{
    padding: 1% 4%;
    margin: 10px;
    border: 1px solid #E5E5EE;
    border-radius: 100px;
    display: inline-block;
    font-size: 28px;
    color: #8E96AB;
    letter-spacing: 0;
    text-align: center;
    margin: 5% 0 3.5% 5%;
}
.isactive{
    border: 1px solid #203260;
    color: #203260;
}
.applicationContent{
    height: 84%;
    position: relative;
    overflow: scroll;
}
