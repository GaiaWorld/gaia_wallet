.ga-top-banner{
    background-color: #ccc;
    width: 100%;
    height: 50px;
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
}
.ga-back{
    position: absolute;
    left: 10px;
    cursor: pointer;
}
.ga-banner-title{
    font-size: 20px;
    color: #fff;
}