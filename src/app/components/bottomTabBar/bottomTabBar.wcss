.ga-bottom-tab-bar-container{
    display: flex;
    height: 80px;
    width: 100%;
    position: absolute;
    bottom: 0;
    left: 0;
}
.ga-tab-bar-item{
    flex: 1 0 0;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    background-color: #eee;
    cursor: pointer;
}
.ga-tab-bar-item-active{
    background-color: #ccc;
}
.ga-tab-bar-icon{
    width: 28px;
    height: 28px;
}
.ga-tab-bar-text{
    font-size: 18px;
}